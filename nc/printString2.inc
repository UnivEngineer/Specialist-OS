; Печать только реальных символов, чтобы не портить экран при
; неотформатированном или повреждённом диске.

printString2:

		push	psw
		push	d
		push	b
		push	h

		mov		a, m
		call	printChar_real

		pop		h
		pop		b
		pop		d
		pop		psw

		inx		h
		dcr		b
		jnz		printString2

		ret

