;----------------------------------------------------------------------------
; MXOS NC.COM
; Копирвоание 3-х байт из адреса de в адрес hl
;
; 2013-12-18 Дизассемблировано vinxru
;----------------------------------------------------------------------------

memcpy_hl_de_3:
        ld    a, (de)
        ld    (hl), a
        inc   hl
        inc   de

        ld    a, (de)
        ld    (hl), a
        inc   hl
        inc   de

        ld    a, (de)
        ld    (hl), a
        inc   hl
        inc   de

        ret

;----------------------------------------------------------------------------
; MXOS NC.COM
; Заполнение памяти по адресу hl байтом a длинной c
;
; 2013-12-18 Дизассемблировано vinxru
;----------------------------------------------------------------------------

memset_hl_a_c:
        ld      (hl), a
        inc     hl
        dec     c
        jp nz,  memset_hl_a_c
        ret
