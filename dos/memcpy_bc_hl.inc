;+---------------------------------------------------------------------------
; MXOS
; Скопировать блок памяти
;
; На входе
;  hl - откуда начало
;  de - откуда конец + 1
;  (de-hl) - длина
;  bc - куда
;
; На выходе
;  de - сохраняется
;
; 2013-12-12 Дизассемблировано vinxru
;----------------------------------------------------------------------------

memcpy_bc_hl:
            ld      a, (hl)
            ld      (bc),a
            inc     hl
            inc     bc
            call    cmp_hl_de
            jp nz,  memcpy_bc_hl
            ret

