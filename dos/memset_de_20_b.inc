;+---------------------------------------------------------------------------
; MXOS
; Заполнить память пробелами
;
; На входе
;  de - адрес
;  b  - длина (0 считается за 256)
;
; 2013-12-12 Дизассемблировано vinxru
;----------------------------------------------------------------------------

memset_de_20_b:
            ld      a, ' '
memset_de_a_b:
            ld      (de), a
            inc     de
            dec     b
            jp nz,  memset_de_a_b
            ret

