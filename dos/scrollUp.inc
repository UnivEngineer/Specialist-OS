;+---------------------------------------------------------------------------
; MXOS
; Прокрутка экрана вверх
;
; На выходе
;   af, bc - сохраняются
;
; 2013-12-12 Дизассемблировано vinxru
;----------------------------------------------------------------------------

            ; Сохраняем регистры
            push  bc
            push  af

            ; Сохраняем sp
            ld    hl, 0
            add   hl,sp
            ld    (bios_vars.oldSP), hl

            ; Прокрутка экрана вверх
            ld    sp, 900Ah   ; Откуда
            ld    h, 90h      ; Куда
            ld    b, 48       ; Столбцов
            jp    scrollUp2

